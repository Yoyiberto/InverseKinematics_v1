            <variable name="MatMul">
              <type>
                <derived name="MatMul" />
              </type>
            </variable>
            <variable name="Exp_se3_fromTrans">
              <type>
                <derived name="Exp_se3_fromTrans" />
              </type>
            </variable>
            <variable name="se3ToSpatialVel">
              <type>
                <derived name="se3ToSpatialVel" />
              </type>
            </variable>
            <variable name="norm">
              <type>
                <derived name="norm" />
              </type>
            </variable>
            <variable name="i0">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="i">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="i1">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="j1">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="maxiterations">
              <type>
                <DINT />
              </type>
            </variable>
