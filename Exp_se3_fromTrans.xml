        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">TransSE3_ToRotAndPos(T:= T,
	 RotMat=&gt;R_mat , p=&gt;p );
	 
Exp_so3_fromRot(Rot:= R_mat, 
	so3mat=&gt; omgmat);
	
equalsZero:=TRUE;i:=1;j:=1;	
FOR i:=1 TO 3 DO
	FOR j:=1 TO 3 DO
		IF (omgmat[i,j]&lt;&gt;0) THEN
			equalsZero:=FALSE;
		END_IF
	END_FOR
END_FOR	

IF equalsZero=TRUE THEN

	i:=1;j:=1;	
	FOR i:=1 TO 3 DO
		FOR j:=1 TO 3 DO
			expmat[i,j]:=0;
		END_FOR
	END_FOR	

	expmat[1,4]:=T[1,4];
	expmat[2,4]:=T[2,4];
	expmat[3,4]:=T[3,4];
	
	expmat[4,1]:=0;
	expmat[4,2]:=0;
	expmat[4,3]:=0;
	expmat[4,4]:=0;

ELSE
	Trace(Matrix:= R_mat,
	 trace=&gt; trace_R);

    theta := ACOS((trace_R - 1) / 2);
	
	i:=1;j:=1;	
	FOR i:=1 TO 3 DO
		FOR j:=1 TO 3 DO
			expmat[i,j]:=omgmat[i,j];
		END_FOR
	END_FOR	

	
	Eye(eye:= eye3);
	Mat_byScalar(scalar:=0.5 , Matrix:= omgmat,
		 MatrixByScalar:= temp_p2);
		
	temp_scalar:=(1 / theta - 1/(TAN(theta / 2)) / 2)/ theta;
	MatMul(mat1:=omgmat , mat2:=omgmat , 
		MatResult:=omgmat2 );	
		
	Mat_byScalar(Matrix:= omgmat2, scalar:=temp_scalar ,
		 MatrixByScalar:=temp_p3 );	
	i:=1;j:=1;	
	FOR i:=1 TO 3 DO
		FOR j:=1 TO 3 DO
			temp_p4[i,j]:=eye3[i,j]-temp_p2[i,j]+temp_p3[i,j];
		END_FOR
	END_FOR	
	
	//3x3 by 3x1vector = 3x1
	i:=1;j:=1;	
	FOR i:=1 TO 3 DO
		expmat[i,4]:=0;
		FOR j:=1 TO 3 DO
			expmat[i,4]:=expmat[i,4]+temp_p4[i,j]*p[j];
		END_FOR
	END_FOR	
	
	expmat[4,1]:=0;
	expmat[4,2]:=0;
	expmat[4,3]:=0;
	expmat[4,4]:=0;
END_IF	
	</xhtml>
          </ST>
        </body>
