              <type>
                <REAL />
              </type>
            </variable>
            <variable name="temp_p3">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="temp_p4">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
