    <pous>
      <pou name="TransSE3_ToRotAndPos" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="T">
              <type>
                <array>
                  <dimension lower="1" upper="4" />
                  <dimension lower="1" upper="4" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml">transform</xhtml>
              </documentation>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="RotMat">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml">rotation matrix</xhtml>
              </documentation>
            </variable>
            <variable name="p">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml">position array</xhtml>
              </documentation>
            </variable>
          </outputVars>
          <localVars>
            <variable name="i">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="j">
              <type>
                <DINT />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">i:=1;j:=1;
FOR i:=1 TO 3 DO
	FOR j:=1 TO 3 DO
		RotMat[i,j]:=T[i,j];//equals to RotMatrix
	END_FOR
END_FOR

p[1]:=T[1,4];
p[2]:=T[2,4];
p[3]:=T[3,4];</xhtml>
          </ST>
        </body>
