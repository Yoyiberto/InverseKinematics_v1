              <type>
                <array>
                  <dimension lower="1" upper="4" />
                  <dimension lower="1" upper="4" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="T_inside">
              <type>
                <array>
                  <dimension lower="1" upper="6" />
