                  <dimension lower="1" upper="4" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml">4x4 TransformMatrix?</xhtml>
              </documentation>
            </variable>
            <variable name="T_MatResult">
              <type>
                <array>
                  <dimension lower="1" upper="4" />
                  <dimension lower="1" upper="4" />
                  <baseType>
