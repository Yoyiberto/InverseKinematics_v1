    <pous>
      <pou name="fastInverseTransform" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="T">
              <type>
                <array>
                  <dimension lower="1" upper="4" />
                  <dimension lower="1" upper="4" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="invT">
              <type>
                <array>
                  <dimension lower="1" upper="4" />
                  <dimension lower="1" upper="4" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
          </outputVars>
          <localVars>
            <variable name="TransSE3_ToRotAndPos">
              <type>
                <derived name="TransSE3_ToRotAndPos" />
              </type>
            </variable>
            <variable name="transpose">
              <type>
                <derived name="transpose" />
              </type>
            </variable>
            <variable name="MatMul">
              <type>
                <derived name="MatMul" />
              </type>
            </variable>
            <variable name="RotMat">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="RotMatT">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="p">
              <type>
                <array>
                  <dimension lower="1" upper="3" />
                  <baseType>
                    <REAL />
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="i">
              <type>
                <DINT />
              </type>
            </variable>
            <variable name="j">
              <type>
                <DINT />
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">TransSE3_ToRotAndPos(T:= T, 
	RotMat=&gt; RotMat, p=&gt; p);
	
transpose(B:= RotMat, Bt:= RotMatT);
i:=1;j:=1;
FOR i:=1 TO 3 DO
	FOR j:=1 TO 3 DO
		invT[i,j]:=RotMatT[i,j];
	END_FOR
END_FOR
// product 3x3 by 3x1
i:=1;j:=1;
FOR i:=1 TO 3 DO
	invT[i,4]:=0;
	FOR j:=1 TO 3 DO
		invT[i,4]:=invT[i,4]-RotMatT[i,j]*p[j];
	END_FOR
END_FOR

invT[4,1]:=0;
invT[4,2]:=0;
invT[4,3]:=0;
invT[4,4]:=1;</xhtml>
          </ST>
        </body>
